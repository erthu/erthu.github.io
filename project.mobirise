              "@media (max-width: 768px)": {
                "display": "none"
              },
              "display": "flex",
              "justify-content": "center",
              "button": {
                "height": "100%",
                "width": "(@thumbsWidth * 1px)",
                "margin-right": "1rem",
                "&:nth-last-child(1)": {
                  "margin-right": "0"
                },
