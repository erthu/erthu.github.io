                "width": "100%"
              },
              "amp-img,img": {
                "& when (@imgPlace)": {
                  "object-fit": "contain"
                },
                "& when not (@imgPlace)": {
                  "object-fit": "cover"
                },
                "@media (max-width: 992px)": {
                  "height": "auto!important",
                  "width": "100%!important"
                }
              }
            },
